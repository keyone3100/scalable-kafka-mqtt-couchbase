{
    "name": "@fdj/shared",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "build": "tsc"
    },
    "dependencies": {
        "kafkajs": "^2.2.4"
    },
    "devDependencies": {
        "@types/node": "^20.19.13",
        "typescript": "^5.8.3"
    },
    "exports": {
        "./enums/*": {
            "import": "./dist/enums/*.js",
            "types": "./dist/enums/*.d.ts"
        },
        "./helpers/*": {
